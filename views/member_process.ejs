<div id="member-list">
    <table class="table">
        <thead class="thead-dark">
            <tr>
                <th scope="col">#</th>
                <th scope="col">FULL NAME</th>
                <th scope="col">E-mail</th>
                <th scope="col">işlemler</th>
            </tr>
        </thead>
        <tbody>
            <% members.forEach(member=> { %>
                <tr>
                    <th scope="row">1</th>
                    <td>
                        <%- member.member_fullName %>
                    </td>
                    <td>
                        <%- member.member_email %>
                    </td>
                    <td><button class="btn btn-danger">üye sil</button></td>
                </tr>
                <% }) %>
        </tbody>
    </table>



    <textarea name="" id="" cols="30" rows="10">
                <a href="www.brsyazilim.net">en yeni gönderiler için tıklayın</a>
            </textarea>
    <button id="mailto-members">MAİL GÖNDER</button>
</div>